{
  "name": "cypress-basic",
  "version": "1.0.0",
  "description": "Basic of cypress testing",
  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint cypress/**/*.js --fix",
    "lint": "eslint cypress/**/*.js",
    "cy:open": "npx cypress open",
    "cy:e2e": "cypress run --record --tag 'full'",
    "cy:e2e-smoke": "cypress run --record --tag 'smoke' --env grepTags=@smoke",
    "test": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/januariod/cypress-basic.git"
  },
  "keywords": [
    "cypress",
    "basic",
    "automation"
  ],
  "author": "Daniel Januario",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/januariod/cypress-basic/issues"
  },
  "homepage": "https://github.com/januariod/cypress-basic#readme",
  "devDependencies": {
    "cypress": "^10.3.1",
    "cypress-grep": "^2.6.1",
    "eslint": "^8.20.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.1"
  }
}
